execute pathogen#infect()

syntax on
filetype plugin indent on
colorscheme molokai
set wildmenu

set foldenable
set foldlevelstart=10
set foldnestmax=10
nnoremap <space> za
set foldmethod=indent 

set number

set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4

set cursorline
set showmatch

set colorcolumn=80
